public interface SqlLang {
	String SELECT_ALL_MEMBER = "select * from member order by id";
	
	Connection connect();
	void close(Connection con, PreparedStatement pstmt);
	void close(Connection con, PreparedStatement pstmt, ResultSet rs);
}
